{
  "parser": "babel-eslint",
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "no-unused-expressions": [ 2,{
      "allowShortCircuit": true,
    }],
    "max-len": 0
  },
  "extends": "airbnb"
}